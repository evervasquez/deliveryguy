@import "nib"
@import "normalize.styl"
@import "fonts.styl"


anaranjado =  #e84207
celeste= #1fa392
azul = #042a34
gris = #3c3f35 
gris_claro = #f2f2f2
roboto= 'Roboto Slab', serif
border_caja = #DDDDDD
border_hover= #F5F5F5

.Body-overlay
  display none
  position fixed
  width 100%
  height 100%
  top 0
  bottom 0
  left 0
  z-index 999
  background-color rgba(0,0,0,.7)
  
.SmartOverlay
  display none
  position fixed
  width 100%
  height 100%
  top 0
  bottom 0
  left 0
  z-index 999
  background-color rgba(0,0,0,.7)

.OLRegister
  background rgba(255,255,255,0.8)
  background-size cover
  background-position center
  background-repeat no-repeat
  width 45%
  margin 10em auto
  text-align center
  border-radius .5em

.OLRegister-formContainer,.OLRegister-network
  display inline-block

.OLRegister-network
  vertical-align top
  margin-top 3.4em
  margin-left 2em

.OLRegister-title
  color #023844
  font-family 'Roboto Slab', serif
  font-weight bold
  font-size 1.5em
  display inline

.OLRegister-input
  display block
  padding 1em
  border-radius .2em
  border none
  font-family 'Roboto Slab', serif
  color #023844
  font-size 1.2em
  margin 0 auto 1em
  -webkit-transition all 0.5s ease
  &:focus
    -webkit-box-shadow 0 4px #023844 inset
    -moz-box-shadow 0 0 4px #023844 inset
    -ms-box-shadow 0 0 4px #023844 inset
    -o-box-shadow 0 0 4px #023844 inset
    box-shadow 0 0 4px #023844 inset
    background #ffffc6
 

.OLRegister-send
.OLRegister-cancel
  color #023844
  background #a1c9c5
  padding .5em 1em
  border none
  margin-left 2em
  font-weight bold
  border-radius .2em
  border-bottom 4px solid #1db1a3
  margin-bottom 2em
  font-family 'Roboto Slab', serif

.OLRegister-send
  &:hover
    color white
    background #e84207
    border-bottom 4px solid #023844

.OLRegister-facebookLink, .OLRegister-googleLink,
.OLRegister-twitterLink
  display block
  color white
  padding 1em 3em
  text-decoration none
  font-size 1em

.OLRegister-close
  float right
  color #e84207
  text-decoration none
  margin 1em 1em 0 0
  &:hover
    color #023844

.OLRegister-facebookLink
  background #495e90
  &:hover
    background #2f3d5e

.OLRegister-googleLink
  background #d3402a
  &:hover
    background #a83322

.OLRegister-twitterLink
  background #55acee
  &:hover
    background #1c91e8

.OLRegister-icon
  font-size 1.4em
  margin-right .5em

.Header
  background rgba(255,255,252,1)
  position fixed
  left 0
  right 0
  display table
  z-index 2
  border-bottom 2px solid #023844

.Header-logoLink
  text-decoration none

.Header-containerImage
  margin 0
  padding 1em 0 0 0.5em
  width 9%
  display table-cell
  vertical-align middle

.Header-image
  padding 0
  text-decoration none
  width 100%

.MainMenu
  display table-cell
  vertical-align middle
  padding-left 3em
  margin 0

.MainMenu-list
  margin 0
  padding 0

.MainMenu-itemCont
  float right
  margin-right 1em

.MainMenu-item
  display inline-block

.MainMenu-link
  text-decoration none
  color #333c4e
  font-size 0.9em
  &:hover
    color #e84207

.MainMenu-itemSignIn
  display inline-block
  font-weight bold
  text-transform uppercase

.MainMenu-appStore,.MainMenu-google
  width 10%
  text-decoration none

.MainMenu-linkSignIn
  text-decoration none
  border 3px solid #023844
  display block
  padding 0.5em
  color #444444

.InfoPrincipal
  overflow hidden
  text-align center
  height 45em
  background url('../img/miami2.jpg')
  background-size cover
  background-position center
  background-repeat no-repeat

.InfoPrincipa-phone
  display inline-block
  margin-right 3em



.InfoPrincipal-image
  width 94%
  margin-top 10em

.InfoPrincipal-containerDescription
  vertical-align top
  text-align left
  display inline-block
  padding-top 11em
  width 60%

.InfoPrincipal-title
  color #e84207
  font-family 'Roboto Slab', serif
  font-weight 300
  font-size 4em
  padding 0

.InfoPrincipal-marca
  font-weight bold
  font-family 'Roboto Slab', serif

.InfoPrincipal-subtitle
  color #023844
  font-size 1.3em
  font-weight bold
  font-family 'Roboto Slab', serif

.InfoPrincipal-more
  color #023844
  text-align right
  &:hover
    color #e84207

.InfoPrincipal-assoContainer

.InfoPrincipal-linkContainer
  display inline-block

.InfoPrincipal-link
  display block
  background #023844
  padding 1em
  border-radius .5em
  color white
  text-decoration none
  border-bottom 5px solid #1db1a3
  margin-right 1em
  font-family 'Roboto Slab', serif
  &:hover
    background #e84207
    border-bottom 5px solid #023844

.RequestDelivery
  background white

.RequestDelivery-container
  text-align center
  padding-top 2em
  width 100%
  

.RequestDelivery-content
  width 43%
  text-align left
  display inline-block
  vertical-align top

.link-derecho
  text-align center

.RequestDelivery-title
  vertical-align top
  font-family 'Roboto Slab', serif
  font-size 4em
  font-weight lighter
  margin 0
  color #023844

.RequestDelivery-description
  display inline-block
  vertical-align top
  line-height 2em
  font-size 1.3em
  color #333c4e

.RequestDelivery-imageContainer
  width 45%
  display inline-block
  margin 0

.RequestDelivery-image
  float right
  width 90%
  height auto
  border-radius 50%
  vertical-align bottom

.TrackingDelivery
  position relative
  overflow hidden

.TrackingDelivery-content
  position absolute
  top 0
  right 0
  width 35%
  margin 1em 4em 0 0
  background white
  border-radius 0.7em
  padding-left 1em

#map_canvas
  height 450px
  opacity .4
 
.TrackingDelivery-title
  font-family 'Roboto Slab', serif
  font-size 3em
  font-weight lighter
  margin 0
  color #e84207

.TrackingDelivery-description
  line-height 2em
  font-size 1.3em
  color #333c4e

.ReceiveDelivery-article
  height 29em
  position relative
  background white
  overflow visible

.ReceiveDelivery-content
  width 40%
  text-align left
  margin 3.2em 0 0 5em
  display inline-block

.ReceiveDelivery-imageContainer
  margin 0
  display inline-block
  width 40%
  position absolute 
  bottom 0
  right 0

.ReceiveDelivery-image
  width 100%
  

  

.ReceiveDelivery-title
  font-family 'Roboto Slab', serif
  font-size 4em
  font-weight lighter
  margin 0
  color #023844

.ReceiveDelivery-description
  line-height 2em
  font-size 1.3em
  color #333c4e

.Footer
  height 8em
  background-color #023844
  text-align center
  padding-top 5em

.Footer-containerDes
  padding 0
  display inline-block
  padding-left 5em

.Footer-title
  color rgba(255,255,255,0.4)
  margin 0
  display inline-block
  padding 0
  font-size .9em

.Footer-networkContainer
  margin 0
  padding 0
  display inline-block
  float right

.Footer-network
  color white
  text-decoration none
  font-size 1.2em
  margin-right .2em
  display block
  background #1db1a3
  padding .2em
  border-radius .2em
  &:hover
    background #e84207

.Footer-corporation
  color rgba(255,255,255,0.4)
  display inline-block
  margin 0
  font-size .9em

.Footer-link
  color rgba(255,255,255,0.4)
  text-decoration none

.none
  display none

.MenuSmart 
  display none

