          text-align center
          background azul
          width 70%
          margin .5em auto
          padding 0
          border 1px inset white
          
@media screen and (max-width:425px)
  .RequestDelivery
    &-imageContainer
      width 60%
      top 15%
      left 20%
    &-image
      width 100%
  .ReceiveDelivery
    
    &-imageContainer
      width 50%

@media screen and (max-width:396px) 
  .RequestDelivery
    &-title
      font-size 2.2em
@media screen and (max-width:320px)     
   .RequestDelivery
    &-title
      font-size 2em
    &-description
      padding 10em .5em 0
      
// **********Profile**********
.profile-body
  background-color gris_claro
.profile-menu
  display none

.profile
  position relative
  margin-top 3.5em
  z-index 1
  &__container
    background-color celeste
    text-align center
  &__name
    margin 0 auto
    padding 0
    color white
    font-family roboto
    font-weight lighter
  &__imgPerfilConteiner
    display inline-block
    width 35%
    margin 1rem 0 1rem 0
  &__imgPerfil
    border-radius 50%
    width 100%
    
  &__imgMotoConatiner
    margin 0
    position absolute
    top 0 
    right 0
  &__imgMoto
    margin .3rem .3rem 0 0
    
.profile-info 
  margin 1rem
  border-radius .7rem
  box-shadow: 3px 3px 20px #888888;
  border 1px solid border_caja
  background-color white
  
 
  &__container
  
  &__title
    font-family roboto
    font-size 1rem
    text-align center
    font-weight bold
    text-transform uppercase
    color gris
  &__list
    list-style none
    margin 0
    padding 0
    text-align left
    border 1px solid border_caja
  &__item
    text-transform none
    border-bottom 1px solid border_caja
    text-decoration none
    color gris
    display block
    padding 1rem 0 1rem 1rem  
    &:hover
      background-color border_hover
  
   
  &__icon
    margin-right .5rem
    padding-left 1rem
    &.fa
      font-size 1.4rem
  &__datos
    font-size 1.2rem
    padding-left 1rem
    text-decoration none
    font-weight normal

@media screen and (min-width:801px)
  .contend-notebook
    display flex
    align-items initial
  .profile-menu   
    display block
    flex-grow 1
    .profile-info    
      &__list
        height 110%vh
      &__item
        padding .2rem 0 .2rem 1rem  
      &__link
        text-decoration none
        display block 
        padding .8rem
        color gris
  .profile-content
    flex-grow 3
    .profile
      margin-top 1rem
      &__container
        border-radius .5rem

.right
  text-align right
.send
  width 80% 
  margin 0
  margin 3rem 0 3rem 0
.register
  margin 0 auto
  margin-top 3.5rem
  display flex
  justify-content center
  margin-bottom 25vh
  margin-top 12rem 
  &__google
  &__facebook
    display block
    padding 1rem 2rem 1rem 2rem
    color white
    text-decoration none
    span
      margin-left .5rem
  &__facebook
    background #3F5D9A
   
  &__google
    background #C43B2B
    margin-right .5rem
  &__tab 
    background #294b53
    display flex
    align-items center
    width 22%
  &__list
    padding 0
    margin-left .2rem  
    width 100%
  &__item
    list-style none
    width 100%
    &:last-child
      width 90%
  &__link
    display block
    background anaranjado 
    padding 1.5rem 3rem 1.5rem .5rem
    font-family roboto
    font-weight lighter
    font-size 1rem
    margin .4rem 0 0 0  
    text-decoration none
    color white

    &--white
      background white
      color celeste 
      padding-right 8rem
    
  &__link--margin
      margin-right .5rem
  

  &__resta
    text-align center
    background-color white
    width 30vw

  &__form
    text-align center
    background-color white
    width 30vw
    display none
  &__title
    font-family roboto
    text-transform uppercase  
    color azul 
  .form
    text-align left
    margin-left 2rem
    width 100%
    border none
  
  &__titleInput
    font-family roboto
    margin-left .8rem
  &__contInput
    display table
    margin-bottom 1rem
    .input-group-addon
      background #EEEEEE
      color #333333
      padding .7rem .6rem .7rem .6rem
      margin 0
      border 1px solid #cccccc
      border-radius .3rem
      border-right 0
      border-bottom-right-radius 0
      border-top-right-radius 0
      white-space nowrap
      display table-cell
      vertical-align middle

  &__input
    background white
    border 1px solid #cccccc
    border-radius .3rem
    padding .7rem 0 .57rem .6rem
    border-bottom-left-radius 0
    border-top-left-radius 0
    display table-cell
    vertical-align middle
    width 20vw
    &:focus
      outline  none
      border-color celeste
      box-shadow 0 0 10px celeste
    
  &__btnSend 
    
    background celeste
    color white
    font-family roboto
    font-weight lighter 
    padding .7rem
    border 0
    border-radius .3em
    &:hover
      background #249284
  
  &__btnCancel
    display inline-block
    padding .7rem
    font-family roboto
    background-color gris 
    color white
    text-decoration none
    border-radius .3em
    margin-left 1rem
    
@media screen and (max-width:824px)
  .register
    display block
    &__tab
      width 100%
      height 3.34rem
    &__item 
      width 49%
      display inline-block
      &:last-child
        width 49%
    &__form
      width 100%
      margin 0
      
    &__resta
      width 100%
    &__title
      margin 0
      padding-top 3rem
    &__list
      margin 0
      text-align center 
    &__link
      padding 1rem 0 1rem 0
      font-size .7rem
      
    &__input
      width 70vw
  .right
    text-align center
    padding-bottom 2rem
  .send
    margin 0
      
    
    
   
    
       

    
  
  
    
    
     
    

    
    

    
    
    
  

  
   



  

          
      
      
      
      
  
 
      
   

  
      



     


      
      
    






















































